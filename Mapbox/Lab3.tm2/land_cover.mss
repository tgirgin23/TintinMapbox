#landcover
{
  [class='wood'] 
  { 
    polygon-fill: darkseagreen; 
    polygon-opacity: 0.7;
  }
  [class='scrub'] 
  { 
    polygon-fill: mix(darkseagreen,cornsilk,75%); 
    polygon-opacity: 0.6;
  }
  [class='grass']
  { 
    polygon-fill: mix(darkseagreen,cornsilk,50%); 
    polygon-opacity: 0.6;
  }
}